{
    "name": "tshafer/laravel-flash",
    "description": "Flash messages for Laravel 5.",
    "require": {
        "php": ">=5.5.9",
        "tshafer/laravel-service-provider": "1.*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://toran.tjshafer.com/repo/private/"
        },
        {
            "type": "composer",
            "url": "http://toran.tjshafer.com/repo/packagist/"
        },
        {
            "packagist": false
        }
    ],
    "autoload": {
        "psr-4": {
            "Tshafer\\Flash\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.*"
        }
    }
}
